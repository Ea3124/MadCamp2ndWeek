{
    "name": "template-svelte",
    "description": "A Phaser 3 project template that demonstrates Svelte communication and uses Vite for bundling.",
    "version": "1.1.2",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phaserjs/template-svelte.git"
    },
    "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/phaserjs/template-svelte/issues"
    },
    "homepage": "https://github.com/phaserjs/template-svelte#readme",
    "keywords": [
        "phaser",
        "phaser3",
        "svelte",
        "vite"
    ],
    "scripts": {
        "dev": "node log.js dev & vite --config vite/config.dev.mjs --host 0.0.0.0",
        "build": "node log.js build & vite build --config vite/config.prod.mjs",
        "dev-nolog": "vite --config vite/config.dev.mjs",
        "build-nolog": "vite build --config vite/config.prod.mjs",
        "preview": "vite preview --outDir build",
        "migrate": "node server/migration.cjs",
        "start": "node server/server.cjs"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^3.2.0",
        "@sveltejs/adapter-static": "^3.0.1",
        "@sveltejs/kit": "^2.5.8",
        "@sveltejs/vite-plugin-svelte": "^3.1.0",
        "@types/pg": "^8.11.10",
        "rollup": "^4.42.0",
        "svelte": "^4.2.17",
        "svelte-check": "^4.0.2",
        "terser": "^5.31.0",
        "ts-node-dev": "^2.0.0",
        "tslib": "^2.6.2",
        "typescript": "^5.4.5",
        "vite": "^5.4.19"
    },
    "type": "module",
    "dependencies": {
        "cookie": "^1.0.2",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "pg": "^8.13.1",
        "phaser": "^3.87.0",
        "phaser-matter-collision-plugin": "^1.0.0",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-arm64-gnu": "^4.42.0"
    }
}
